*Easy*
6) --Iterative--

   ListNode* reverseList(ListNode* head) {
     ListNode* prev=NULL, *curr=NULL, *later=head;
        while(later)
        {
          prev=curr;
          curr=later;
          later=later->next;
          curr->next=prev;
        }
        return curr;
    }
   
   --Recursive--
   
    ListNode* reverseList(ListNode* head) {
    if(head==NULL || head->next==NULL)
        return head;
    ListNode* tail= head->next;
    ListNode* newHead= reverseList(head->next);
    tail->next=head;
    head->next=NULL;
    return newHead;
    }
   
 7)  --Iterative--
 
  ListNode* mergeTwoLists(ListNode* list1, ListNode* list2) {
    ListNode* head= new ListNode(0);
    ListNode* curr= head;
    while(list1 && list2)
    {
        if(list1->val <= list2->val)
        {
            curr->next=list1;
            list1=list1->next;
        }
        else
        {
            curr->next= list2;
            list2=list2->next;
        }
        curr=curr->next;
    }
        if(list1)
        {
          curr->next= list1;
        }
        if(list2)
        {
         curr->next=list2;
        }
    return head->next;
    }
  
  --Recursive--
  
   ListNode* mergeTwoLists(ListNode* list1, ListNode* list2) {
    if(list1==NULL)
        return list2;
    if(list2==NULL)
        return list1;
    if(list1->val < list2->val)
    {
        list1->next = mergeTwoLists(list1->next, list2);
        return list1;
    }
    else
    {
        list2->next= mergeTwoLists(list1, list2->next);
        return list2;
    }
   }     
  
8) ListNode* deleteDuplicates(ListNode* head) {
    ListNode* temp= head;
    while(temp && temp->next)
     {
       if(temp->val == temp->next->val)
       {
           temp->next=temp->next->next;
       }
     else
        temp=temp->next;
     }
        return head;
    }
    
    
 9)  ListNode *getIntersectionNode(ListNode *headA, ListNode *headB) {
      ListNode* a=headA;
      ListNode* b=headB;
        while(a!=b)
        {
            if(a==NULL)
             a=headB;
            else
             a=a->next;
            if(b==NULL)
             b=headA;
            else
             b=b->next;
        }
        return a;
    } 
    
    
10) bool hasCycle(ListNode *head) {
     ListNode *fast=head, *slow=head;
    while(fast && fast->next)
    {
        slow=slow->next;
        fast=fast->next->next;
        if(slow==fast)
            return 1;
    }
        return 0;
    }
    
    
11) --Iterative--

     ListNode* reverse(ListNode* head)
    {
        ListNode *prev=NULL, *curr=head, *later=head;
        while(curr)
        {
            later=curr->next;
            curr->next=prev;
            prev=curr;
            curr=later;
        }
       return prev; 
    }
    bool isPalindrome(ListNode* head) {
     ListNode *slow=head, *fast=head;
     while(fast && fast->next)
     {
         slow=slow->next;
         fast=fast->next->next;
     }
     slow=reverse(slow);
     ListNode* temp=head;
    while(slow)
    {
      if(temp->val!=slow->val)
          return 0;
        temp=temp->next;
        slow=slow->next;
    }
    return 1;
    }

12) --Iterative--
 
    ListNode* removeElements(ListNode* head, int val) {
    ListNode* curr=head;
    ListNode* later=NULL;
    while(head && head->val==val)
    {
        head=head->next;
    }
    while(curr && curr->next)
    {
        later=curr->next;
        if(later->val==val)
        {
            curr->next=curr->next->next;
        }
        else{
         curr=curr->next;
        }
    }
        return head;
    }
    
    
   --Recursive--
   
    ListNode* removeElements(ListNode* head, int val) {
    if(head==NULL)
        return NULL;
    head->next=removeElements(head->next, val);
    if(head->val==val)
        head=head->next;
    return head; 
 } 
   
   
    
